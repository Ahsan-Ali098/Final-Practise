<h1>User Form </h1>
<%= form_for @user do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name %><br>
  <br>
  <%= f.label :Occupation %><br>
  <%= f.text_area :occupation %><br>
  <br>
  <%= f.label :email %><br>
  <%= f.text_area :email %><br>
  <br>
  <%= f.submit %>

<% end %>